{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-console": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
